{
  "version": 2,
  "name": "subway-time",
  "alias": ["subwayti.me"],
  "builds": [
    {
      "src": "web/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/station/.*", "dest": "web/index.html" },
    { "src": "/(.*)", "dest": "web/$1" }
  ]
}
