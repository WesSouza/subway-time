{
  "name": "subway-time-web",
  "version": "1.0.0",
  "homepage": "http://subwayti.me/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesSouza/subway-time"
  },
  "keywords": [
    "subway",
    "time table"
  ],
  "license": "MIT",
  "author": "Wes Souza <hey@wes.dev>",
  "scripts": {
    "build": "parcel build ./src/index.html --public-url '/'",
    "dev": "concurrently -r \"npm:dev-ts\" \"parcel ./src/index.html\"",
    "dev-ts": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "now-build": "yarn run build",
    "ts": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "color-contrast": "0.0.1",
    "geolib": "^3.2.1",
    "immer": "^5.1.0",
    "natural-compare-lite": "^1.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0-beta",
    "sanitize.css": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@svgr/parcel-plugin-svgr": "^5.0.1",
    "@types/node": "^13.1.5",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "autoprefixer": "^9.7.3",
    "concurrently": "^5.0.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "postcss-modules": "^1.4.1",
    "typescript": "~3.7.4"
  },
  "engines": {
    "browsers": "last 2 Chrome versions",
    "node": ">= 10.x"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
