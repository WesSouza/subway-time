{
  "name": "subway-time-web",
  "version": "1.0.0",
  "homepage": "http://subwayti.me/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesSouza/subway-time"
  },
  "keywords": [
    "subway",
    "time table"
  ],
  "license": "MIT",
  "author": "Wes Souza <hey@wes.dev>",
  "scripts": {
    "build": "npm run type-check && npm run lint && parcel build ./src/index.html --public-url '/'",
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "lint": "tslint -p .",
    "now-build": "npm run build",
    "prettify": "prettier --write './**/*.{ts,tsx}'",
    "start": "parcel ./src/index.html",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "color-contrast": "0.0.1",
    "geolib": "^2.0.24",
    "natural-compare-lite": "^1.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@svgr/parcel-plugin-svgr": "^4.2.0",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "autoprefixer": "^9.5.0",
    "concurrently": "^4.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1"
  },
  "engines": {
    "browsers": "last 2 Chrome versions",
    "node": ">= 8.10.0",
    "npm": ">= 6.9.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
