{
  "name": "subway-time-web",
  "version": "1.0.0",
  "homepage": "http://subwayti.me/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesSouza/subway-time"
  },
  "keywords": [
    "subway",
    "time table"
  ],
  "license": "MIT",
  "author": "Wes Souza <hey@wes.dev>",
  "scripts": {
    "build": "parcel build ./src/index.html --public-url '/'",
    "dev": "concurrently -r \"npm:dev-ts\" \"parcel ./src/index.html\"",
    "dev-ts": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "now-build": "yarn run ts && yarn run lint && yarn run build",
    "ts": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "color-contrast": "0.0.1",
    "geolib": "^3.0.4",
    "immer": "^4.0.1",
    "natural-compare-lite": "^1.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "react-helmet": "^6.0.0-beta",
    "sanitize.css": "^11.0.0"
  },
  "resolutions": {
    "react-helmet/react-side-effect": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-react": "^7.6.3",
    "@svgr/parcel-plugin-svgr": "^4.3.3",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-helmet": "^5.0.12",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "autoprefixer": "^9.6.5",
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  },
  "engines": {
    "browsers": "last 2 Chrome versions",
    "node": ">= 8.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
