{
  "name": "subway-time",
  "version": "2.0.0",
  "private": true,
  "workspaces": [
    "api",
    "web"
  ],
  "homepage": "http://subwayti.me/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesSouza/subway-time"
  },
  "keywords": [
    "subway",
    "time table"
  ],
  "license": "MIT",
  "author": "Wes Souza <hey@wes.dev>",
  "scripts": {
    "dev": "concurrently -r -n proxy,api,web \"yarn run dev-server\" \"cd api; yarn run dev\" \"cd web; yarn run dev\"",
    "dev-server": "node ./dev-server.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "got": "^10.5.7",
    "import-sort-style-wes": "^1.0.1",
    "prettier": "1.19.1",
    "prettier-plugin-import-sort": "^0.0.3"
  },
  "engines": {
    "node": ">= 10.x"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "wes"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "wes"
    }
  }
}
