  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.2",
    "@types/reach__router": "^1.0.0",
    "@types/react": "^16.4.4",
    "@types/react-dom": "^16.0.6",
    "babel-jest": "^23.2.0",
    "babel-preset-react-app": "^3.1.2",
